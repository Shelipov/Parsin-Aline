@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Use this area to provide additional information.</p>
@model IEnumerable<CoreParsingSite.Models.Goods>
<table class="table">
    <tr><td>Порядковый номер</td><td>Название</td><td>Цена</td><td>Наличие</td><td>Описание</td><td>ФОТО</td></tr>
    @foreach (var item in Model)
    {
        if (item.Foto == null) { continue; }
        <tr><td>@item.Id</td><td>@item.Name</td><td>@item.Price</td><td>@item.Stock</td><td>@item.Description</td><td>@if (item.Foto != null){<img src="data:image/jpeg;base64,@(Convert.ToBase64String(item.Foto))" />}</td></tr>
    }
</table>